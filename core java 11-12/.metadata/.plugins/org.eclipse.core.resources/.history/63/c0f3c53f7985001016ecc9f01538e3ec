package com.rays.collection.iterator;

import java.util.ArrayList;
import java.util.Iterator;
import java.util.List;

//iterator modern class ke sat use hpta hai
// iterator fail fast hota hai
// collection se data nikal ke iterator me store kaene ke bad new data add nhi hota hai
// exception aati hai

public class TestIterator {

	public static void main(String[] args) {

		List l = new ArrayList();

		l.add(5);
		l.add(43);
		l.add("ram");
		l.add("abhi");
		l.add('a');
		l.add(true);

		System.out.println(l);

		Iterator it = l.iterator();
//	l.add(false);

		while (it.hasNext()) {
			Object o = it.next();
			it.remove();
			System.out.println(o);
			
			//

			System.out.println(l);

			
			System.out.println("-------");
		}
		System.out.println(l);

	}
}
